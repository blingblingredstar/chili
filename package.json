{
  "name": "chili-lib",
  "version": "0.1.0",
  "description": "基于antd封装的业务组件库和工具",
  "keywords": [
    "react",
    "antd lib",
    "chili-lib",
    "react-component",
    "antd component",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/poyiding/chili"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build && node ./scripts/clean-build.js",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "del": "^5.1.0",
    "dumi": "^1.0.33",
    "father-build": "^1.17.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "antd": "^4.2.2"
  },
  "license": "MIT"
}
